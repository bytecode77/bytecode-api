<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:ui="http://schemas.bytecode77.com/2023/xaml/presentation"
	xmlns:ctrl="clr-namespace:BytecodeApi.Wpf.Cui.Controls">
	<Style TargetType="{x:Type ToolTip}">
		<Setter Property="Padding" Value="7,3,7,4" />
		<Setter Property="BorderThickness" Value="1" />
		<Setter Property="BorderBrush" Value="#767676" />
		<Setter Property="Background" Value="#f1f2f7" />
		<Setter Property="Foreground" Value="#575757" />
	</Style>
</ResourceDictionary>