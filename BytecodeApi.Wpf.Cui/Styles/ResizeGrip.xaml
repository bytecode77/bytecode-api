<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:ui="http://schemas.bytecode77.com/2023/xaml/presentation"
	xmlns:ctrl="clr-namespace:BytecodeApi.Wpf.Cui.Controls">
	<Style TargetType="{x:Type ResizeGrip}">
		<Setter Property="OverridesDefaultStyle" Value="True" />
		<Setter Property="Width" Value="20" />
		<Setter Property="Height" Value="20" />
		<Setter Property="Background" Value="Transparent" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type ResizeGrip}">
					<Border BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}" SnapsToDevicePixels="True">
						<Grid>
							<Path Data="M10,4 H11 V5 H10 Z M8,6 H9 V7 H8 Z M10,6 H11 V7 H10 Z M6,8 H7 V9 H6 Z M8,8 H9 V9 H8 Z M10,8 H11 V9 H10 Z M4,10 H5 V11 H4 Z M6,10 H7 V11 H6 Z M8,10 H9 V11 H8 Z M10,10 H11 V11 H10 Z" Fill="#303c6a" Width="16" Height="16" HorizontalAlignment="Center" VerticalAlignment="Center" SnapsToDevicePixels="True" />
							<Path Data="M10,4 H11 V5 H10 Z M8,6 H9 V7 H8 Z M10,6 H11 V7 H10 Z M6,8 H7 V9 H6 Z M8,8 H9 V9 H8 Z M10,8 H11 V9 H10 Z M4,10 H5 V11 H4 Z M6,10 H7 V11 H6 Z M8,10 H9 V11 H8 Z M10,10 H11 V11 H10 Z" Fill="#9f8d94" Width="16" Height="16" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="2,2,0,0" SnapsToDevicePixels="True" />
						</Grid>
					</Border>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
</ResourceDictionary>